#include <stdio.h>

#define SIZE 10   // Size of hash table

int hashTable[SIZE];

// Initialize hash table
void initTable() {
    for (int i = 0; i < SIZE; i++)
        hashTable[i] = -1;
}

// Hash function
int hashFunction(int key) {
    return key % SIZE;
}

// Insert using Quadratic Probing
void insert(int key) {
    int index = hashFunction(key);
    int i = 0;

    while (hashTable[(index + i * i) % SIZE] != -1) {
        i++;

        // Table full condition
        if (i == SIZE) {
            printf("Hash table is full. Cannot insert %d\n", key);
            return;
        }
    }

    int newIndex = (index + i * i) % SIZE;
    hashTable[newIndex] = key;
    printf("Inserted %d at index %d\n", key, newIndex);
}

// Display hash table
void display() {
    printf("\nHash Table:\n");
    for (int i = 0; i < SIZE; i++) {
        if (hashTable[i] == -1)
            printf("Index %d : EMPTY\n", i);
        else
            printf("Index %d : %d\n", i, hashTable[i]);
    }
}

int main() {
    int keys[] = {23, 43, 13, 27, 33};
    int n = sizeof(keys) / sizeof(keys[0]); // number of elements in array

    initTable();

    for (int i = 0; i < n; i++)
        insert(keys[i]);

    display();

    return 0;
}
