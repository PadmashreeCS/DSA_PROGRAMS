// if char is repeted put -1, if not put char ex: a a b c >>>  a -1 b c

#include <stdio.h>
#define SIZE 5

char arr[SIZE];
int top = -1;
char value;

int main() 
{
    for(int i = 0; i < SIZE; i++)
    {
        printf("Enter the Character %d: ", i + 1);
        scanf(" %c", &value);

        int repeated = 0;
        for(int j = 0; j <= top; j++)
        {
            if(arr[j] == value)
            {
                repeated = 1;
                break;
            }
        }

        if(repeated)
        {
            arr[++top] = -1;
            printf("repeated -1\n");
        }
        else
        {
            arr[++top] = value;
            printf("not repeated\n");
        }
    }

    printf("\nArray contents:\n");
    for(int i = 0; i <= top; i++)
    {
        if(arr[i] == -1)
            printf("-1 ");
        else
            printf("%c ", arr[i]);
    }
    printf("\n");

    return 0;
}
