
#include <stdio.h>
#include <string.h>
#define SIZE 100

char str[SIZE];
char re_str[SIZE];
int top=-1;

void pushchar(char ch)
{
    str[++top]=ch;
}

char pop()
{
    char iteam = str[top--];
    return iteam;
}

int main()
{
    printf("Enter the string:");
    scanf("%s",&str);
    
    for(int i=0; i<strlen(str);i++)
    {
        pushchar(str[i]);
    }
    
    for(int i=0;i<strlen(str);i++)
    {
        re_str[i]=pop();
    }
    
    if(strcmp(str, re_str) == 0)
    {
        printf("yes it palindrome");
    }
    
    else
    {
        printf("No it's not palindrome");
    }
    

    return 0;
}
