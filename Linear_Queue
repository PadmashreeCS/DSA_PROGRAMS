//linear Queue
#include <stdio.h>
#include <stdlib.h>
#define SIZE 5
int front = -1;
int rear = -1;
int choice;
int stack[SIZE];
int value;
void insert()
{
    if(rear == SIZE-1)
    {
        printf("Overflow \n");
    }
    else
    {
        if(front == -1)
        {
            front = front + 1;
        }
         printf("Enter the value to insert \n");
         scanf("%d",&value);
         rear = rear + 1;
         stack[rear]=value;
    }

}

void dell()
{
    if(front == -1)
    {
        printf("underflow \n");
    }
    else
    {
        printf("Delete value %d \n",stack[front]);
        front= front +1;
    }
}

void display()
{
    if(front == -1)
    {
        printf("underflow \n");
    }
    else
    {
        for(int i = front;i<=rear;i++)
        {
            printf("%d \n",stack[i]);
        }
    }
}
void main()
{
    while(1)
    {
        printf("1. Insert  2. Delete  3. Display 4. exit \n");
        printf("Enter you choice: ");
        scanf("%d",&choice);
        switch(choice)
        {
            case 1: insert();
            break;
            case 2: dell();
            break;
            case 3: display();
            break;
            case 4: exit(1);
            break;
            default:
                printf("Invalid entry \n");
        }
    }
}
