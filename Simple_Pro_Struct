
#include <stdio.h>
#define SIZE 3

struct student
{
    int id;
    int marks;
    int age;
};


void accept_input(struct student *s1)
{
    printf("Enter student_id: ");
    scanf("%d",&s1->id);
    
    printf("Enter Age: ");
    scanf("%d",&s1->age);
    
    printf("Enter marks: ");
    scanf("%d",&s1->marks);
    
    if(s1->marks<0 || s1->marks>100)
    {
        printf("Invalid Entry of Marks, student id-->%d \n ",s1->id);
    }
    
}

void check_elegible(struct student *s1)
{
    if(s1->marks>=65 && s1->age>=20)
    {
        printf("%d Eligiblev\n",s1->id);
    }
    else
    {
        printf("%d Not elegible \n",s1->id);
    }
}

int main()
{
    
    struct student s[SIZE];
    
    for(int i=0;i<=2;i++)
    {
        accept_input(&s[i]);
    }
    
    for(int i=0;i<=2;i++)
    {
        check_elegible(&s[i]);
    }
    

    return 0;
}
